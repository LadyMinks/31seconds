<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">


    <nl.ladyminks.thirtyoneseconds.presentation.pages.game.TurnPageImpl
        android:id="@+id/turnPageImpl"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/white"
        android:foregroundGravity="fill" />

    <nl.ladyminks.thirtyoneseconds.presentation.pages.game.ReadyPageImpl
        android:id="@+id/readyPageImpl"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/white"
        android:foregroundGravity="fill" />

    <nl.ladyminks.thirtyoneseconds.presentation.pages.game.ResultsPageImpl
        android:id="@+id/resultsPageImpl"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/white"
        android:foregroundGravity="fill"/>

</FrameLayout>